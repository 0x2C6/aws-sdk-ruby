{
  "version": "2013-01-01",
  "metadata": {
    "endpoint_prefix": "cloudsearchdomain",
    "service_class_name": "CloudSearchDomain",
    "service_full_name": "Amazon CloudSearchDomain"
  },
  "plugins": [
    "Seahorse::Client::Plugins::Logging",
    "Seahorse::Client::Plugins::RestfulBindings",
    "Seahorse::Client::Plugins::ContentLength",
    "Aws::Plugins::UserAgent",
    "Aws::Plugins::RetryErrors",
    "Aws::Plugins::GlobalConfiguration",
    "Aws::Plugins::ResponsePaging",
    "Aws::Plugins::JsonProtocol"
  ],
  "operations": {
    "search": {
      "name": "Search",
      "http_method": "GET",
      "http_path": "/2013-01-01/search?format=sdk&pretty=true&cursor={cursor}&expr={expr}&facet={facet}&fq={filter_query}&highlight={highlight}&partial={partial}&q={query}&q.options={query_options}&q.parser={query_parser}&return={return}&size={size}&sort={sort}&start={start}",
      "input": {
        "type": "input",
        "members": {
          "cursor": {
            "type": "string",
            "location": "uri"
          },
          "expr": {
            "type": "string",
            "location": "uri"
          },
          "facet": {
            "type": "string",
            "location": "uri"
          },
          "filter_query": {
            "type": "string",
            "location": "uri"
          },
          "highlight": {
            "type": "string",
            "location": "uri"
          },
          "partial": {
            "type": "boolean",
            "location": "uri"
          },
          "query": {
            "type": "string",
            "required": true,
            "location": "uri"
          },
          "query_options": {
            "type": "string",
            "location": "uri"
          },
          "query_parser": {
            "type": "string",
            "location": "uri",
            "enum": [
              "simple",
              "structured",
              "lucene",
              "dismax"
            ]
          },
          "return": {
            "type": "string",
            "location": "uri"
          },
          "size": {
            "type": "integer",
            "location": "uri"
          },
          "sort": {
            "type": "string",
            "location": "uri"
          },
          "start": {
            "type": "integer",
            "location": "uri"
          }
        }
      },
      "output": {
        "type": "output",
        "members": {
          "status": {
            "type": "structure",
            "serialized_name": "status",
            "members": {
              "timems": {
                "type": "integer",
                "serialized_name": "timems"
              },
              "rid": {
                "type": "string",
                "serialized_name": "rid"
              }
            }
          },
          "hits": {
            "type": "structure",
            "serialized_name": "hits",
            "members": {
              "found": {
                "type": "integer",
                "serialized_name": "found"
              },
              "start": {
                "type": "integer",
                "serialized_name": "start"
              },
              "cursor": {
                "type": "string",
                "serialized_name": "cursor"
              },
              "hit": {
                "type": "list",
                "serialized_name": "hit",
                "members": {
                  "type": "structure",
                  "members": {
                    "id": {
                      "type": "string",
                      "serialized_name": "id"
                    },
                    "fields": {
                      "type": "map",
                      "serialized_name": "fields",
                      "keys": {
                        "type": "string"
                      },
                      "members": {
                        "type": "list",
                        "members": {
                          "type": "string"
                        }
                      }
                    },
                    "highlights": {
                      "type": "map",
                      "serialized_name": "highlights",
                      "keys": {
                        "type": "string"
                      },
                      "members": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "facets": {
            "type": "map",
            "serialized_name": "facets",
            "keys": {
              "type": "string"
            },
            "members": {
              "type": "structure",
              "members": {
                "buckets": {
                  "type": "list",
                  "serialized_name": "buckets",
                  "members": {
                    "type": "structure",
                    "members": {
                      "value": {
                        "type": "string",
                        "serialized_name": "value"
                      },
                      "count": {
                        "type": "integer",
                        "serialized_name": "count"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "suggest": {
      "name": "Suggest",
      "http_method": "GET",
      "http_path": "/2013-01-01/suggest?format=sdk&pretty=true&q={query}&suggester={suggester}&size={size}",
      "input": {
        "type": "input",
        "members": {
          "query": {
            "type": "string",
            "required": true,
            "location": "uri"
          },
          "suggester": {
            "type": "string",
            "required": true,
            "location": "uri"
          },
          "size": {
            "type": "integer",
            "location": "uri"
          }
        }
      },
      "output": {
        "type": "output",
        "members": {
          "status": {
            "type": "structure",
            "serialized_name": "status",
            "members": {
              "timems": {
                "type": "integer",
                "serialized_name": "timems"
              },
              "rid": {
                "type": "string",
                "serialized_name": "rid"
              }
            }
          },
          "suggest": {
            "type": "structure",
            "serialized_name": "suggest",
            "members": {
              "query": {
                "type": "string",
                "serialized_name": "query"
              },
              "found": {
                "type": "integer",
                "serialized_name": "found"
              },
              "suggestions": {
                "type": "list",
                "serialized_name": "suggestions",
                "members": {
                  "type": "structure",
                  "members": {
                    "suggestion": {
                      "type": "string",
                      "serialized_name": "suggestion"
                    },
                    "score": {
                      "type": "integer",
                      "serialized_name": "score"
                    },
                    "id": {
                      "type": "string",
                      "serialized_name": "id"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "upload_documents": {
      "name": "UploadDocuments",
      "http_method": "POST",
      "http_path": "/2013-01-01/documents/batch?format=sdk",
      "input": {
        "type": "input",
        "members": {
          "documents": {
            "type": "blob",
            "required": true,
            "serialized_name": "documents"
          },
          "content_type": {
            "type": "string",
            "required": true,
            "location": "header",
            "serialized_name": "Content-Type",
            "enum": [
              "application/json",
              "application/xml"
            ]
          }
        },
        "payload": "documents"
      },
      "output": {
        "type": "output",
        "members": {
          "status": {
            "type": "string",
            "serialized_name": "status"
          },
          "adds": {
            "type": "integer",
            "serialized_name": "adds"
          },
          "deletes": {
            "type": "integer",
            "serialized_name": "deletes"
          },
          "warnings": {
            "type": "list",
            "serialized_name": "warnings",
            "members": {
              "type": "structure",
              "members": {
                "message": {
                  "type": "string",
                  "serialized_name": "message"
                }
              }
            }
          }
        }
      }
    }
  }
}
